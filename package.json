    "build:structures": "slidev build ./structures/slides.md --out ./dist",
    "dev:structures": "slidev --open ./structures/slides.md",
    "build:ai": "slidev build ./ai-revolution/slides.md --out ./dist",
    "dev:ai": "slidev --open ./ai-revolution/slides.md",
